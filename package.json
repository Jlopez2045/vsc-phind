{
  "name": "vsc-phind",
  "displayName": "Phind",
  "description": "",
  "version": "1.0.0",
  "publisher": "JLT",
  "icon": "resources/icon.png",
  "repository": "https://github.com/Jlopez2045/vsc-phind",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:phindView"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "phind.refresh",
        "title": "Refresh",
        "icon": "$(refresh)"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "phind",
          "title": "Phind",
          "icon": "resources/logo.svg"
        }
      ]
    },
    "views": {
      "phind": [
        {
          "id": "phindView",
          "type": "webview",
          "name": "The AI search engine for developers"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "phind.refresh",
          "when": "view == phindView",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "postcompile": "npm run copy-resources",
    "copy-resources": "copyfiles -u 1 \"./resources/**/*.*\" ./out"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.77.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5"
  }
}
